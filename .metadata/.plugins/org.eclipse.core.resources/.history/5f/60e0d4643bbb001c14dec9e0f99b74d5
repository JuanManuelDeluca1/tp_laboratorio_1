#include <stdio.h>
#include <stdlib.h>

int main(void) {
	puts("!!!Hello World!!!"); /* prints !!!Hello World!!! */
	return EXIT_SUCCESS;
}
int esVocal(char x)
{
	int todoOk;
	x = tolower(x);
	if(x == 'a' || x == 'e' || x == 'i' || x == 'o' || x == 'u')
	{
		todoOk = 1;
	}
	return todoOk;
}
