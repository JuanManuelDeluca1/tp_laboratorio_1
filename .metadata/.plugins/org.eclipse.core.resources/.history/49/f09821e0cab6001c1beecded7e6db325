#include <stdio.h>
#include <stdlib.h>

int main(void)
{
	setbuf(stdout,NULL);
	int x;
	printf("ingrese un numero: ");
	scanf("%d", &x);
	printf("su numero es %d:", x, "\nputa");
	return 0;
}
