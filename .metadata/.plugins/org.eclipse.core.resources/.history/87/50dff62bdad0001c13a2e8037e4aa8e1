#ifndef MARCA_H_
#define MARCA_H_

typedef struct{

    int id;
    char descripcion[20];
    int isEmpty;

}eMarcas;


#endif /* MARCA_H_ */
int inicializarAuto(eMarcas vec[], int tam);
int buscarAutoLibre(eMarcas vec[], int tam, int* pIndex);
void mostrarMarcas(eAuto e);
int listarMarcas(eAuto vec[], int tam);
